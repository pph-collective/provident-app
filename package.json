{
  "name": "provident",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "start": "serve -s dist -p 8080",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "cypress open",
    "lint": "vue-cli-service lint",
    "shp2topo": "scripts/shp2topo.sh",
    "firebase:dev": "firebase emulators:start --import emulator_data",
    "firebase:savedev": "firebase emulators:export emulator_data",
    "add-admin": "node scripts/add-admin.js",
    "add-form": "node scripts/add-form.js",
    "add-results": "node scripts/add-results.js"
  },
  "dependencies": {
    "@formvuelate/plugin-vee-validate": "^2.2.1",
    "aos": "^2.3.4",
    "arquero": "^4.8.4",
    "bulma": "^0.9.2",
    "bulma-responsive-tables": "^1.2.3",
    "core-js": "^3.6.5",
    "firebase": "^8.2.6",
    "formvuelate": "^3.1.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "topojson-simplify": "^3.0.3",
    "vee-validate": "^4.4.0-alpha.2",
    "vega": "^5.19.1",
    "vega-embed": "^6.15.1",
    "vega-lite": "^4.17.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "4",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@cypress/vue": "^3.0.1",
    "@cypress/webpack-dev-server": "^1.3.0",
    "@cypress/webpack-preprocessor": "^5.8.0",
    "@firebase/rules-unit-testing": "^1.3.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "csv-parse": "^4.16.0",
    "cypress": "7.7.0",
    "cypress-firebase": "^1.9.1",
    "d3-dsv": "^2.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "firebase-admin": "^9.8.0",
    "firebase-tools": "^9.3.0",
    "lint-staged": "^9.5.0",
    "ndjson-cli": "^0.3.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.2",
    "shapefile": "^0.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)",
          "**/scripts/*.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
